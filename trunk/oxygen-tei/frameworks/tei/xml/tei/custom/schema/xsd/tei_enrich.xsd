                            <xs:documentation/>
                          </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="sup">
                          <xs:annotation>
                            <xs:documentation/>
                          </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="rubric">
