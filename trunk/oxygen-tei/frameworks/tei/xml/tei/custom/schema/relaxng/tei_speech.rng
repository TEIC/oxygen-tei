        <choice>
          <text/>
          <ref name="model.gLike"/>
          <ref name="model.phrase"/>
          <ref name="argument"/>
          <ref name="byline"/>
          <ref name="dateline"/>
          <ref name="epigraph"/>
          <ref name="salute"/>
          <ref name="signed"/>
