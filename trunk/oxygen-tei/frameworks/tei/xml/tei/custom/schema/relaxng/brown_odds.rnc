boho_moduleRef =
  ## (module reference) references a module which is to be incorporated into a schema.
  element ns1:moduleRef {
    (boho_content?)
    >> sch:pattern [
         id = "moduleRef-constraint-not-key-and-url"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "                  "
           sch:report [
             test = "@url and @key"
             "\x{a}" ~
             "		    only the key= OR the url= attribute of ﹤moduleRef﹥ should be supplied, not both\x{a}" ~
             "		  "
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "moduleRef-constraint-not-include-and-except"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "                  "
           sch:report [
             test = "@except and @include"
             "\x{a}" ~
             "		    only the exclude= OR the include= attribute of ﹤moduleRef﹥ should be supplied, not both\x{a}" ~
             "		  "
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "moduleRef-constraint-if-url-then-prefix"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef[@url]"
           "\x{a}" ~
           "                  "
           sch:assert [
