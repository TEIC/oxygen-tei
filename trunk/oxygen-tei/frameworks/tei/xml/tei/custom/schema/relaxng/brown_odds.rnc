  attribute xml:id { xsd:ID }?
boho_att.global.attribute.n =
  ## (number) gives a number (or other label) for an element, which is not necessarily unique within
  ##         the document.
  attribute n { boho_data.text }?
boho_att.global.attribute.xmllang =
  ## (language) indicates the language of the element content using a tag generated
  ##         according to BCP 47
  attribute xml:lang { boho_data.language }?
boho_att.global.attribute.rend =
  ## (rendition) indicates how the element in question was rendered or presented in the source text.
  attribute rend {
    list { boho_data.word, boho_data.word* }
  }?
boho_att.global.attribute.rendition =
  ## points to a description of the rendering or presentation used for this element in the
  ##         source text.
  attribute rendition {
    list { boho_data.pointer, boho_data.pointer* }
  }?
boho_att.global.attribute.xmlbase =
  ## provides a base URI reference with which applications can resolve relative URI
  ##         references into absolute URI references.
  attribute xml:base { boho_data.pointer }?
boho_att.global.attribute.xmlspace =
  ## signals an intention about how white space should be 
  ##       managed by  applications. 
  attribute xml:space {
    ## the processor should treat white space according to the
    ##	default XML white space handling rules
    "default" | ## the processor should preserve unchanged any and all
      ##	white space in the source
      "preserve"
  }?
boho_att.internetMedia.attributes =
  boho_att.internetMedia.attribute.mimeType
boho_att.internetMedia.attribute.mimeType =
  ## (MIME media type) specifies the applicable multimedia internet mail extension (MIME) media type
