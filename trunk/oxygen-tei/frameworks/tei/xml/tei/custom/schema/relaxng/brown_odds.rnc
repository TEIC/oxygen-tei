      "true" | ## No prefixes are added: 
        ##  any prefix required by the value of the prefix attribute on the current
        ##	  schemaSpec must therefore be supplied explicitly, as appropriate. 
        "false"
    }?,
    empty
  }

boho_constraint =
  ##  (constraint rules) the formal rules of a constraint
  element ns1:constraint {
    (sch_assert | sch_ns | sch_report | sch_rule)+,
    boho_att.global.attributes,
    empty
  }

boho_constraintSpec =
  ##  (constraint on schema) contains  a constraint, expressed in some formal syntax,
  ##   which cannot be expressed in the structural content model [22.4.4. Element Specifications]
  element ns1:constraintSpec {
    boho_model.glossLike*,
    (boho_constraint?)
    >> sch:ns [
         prefix = "s"
         uri = "http://www.ascc.net/xml/schematron"
       ]
    >> sch:pattern [
         id = "constraintSpec-constraint-sch"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/s:* and    not(@scheme='schematron')"
             "\x{a}" ~
             "	Rules in the Schematron 1.* language must be inside\x{a}" ~
             "	a constraint with a value of 'schematron' on the scheme attribute\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:ns [
         prefix = "sch"
         uri = "http://purl.oclc.org/dsdl/schematron"
       ]
    >> sch:pattern [
         id = "constraintSpec-constraint-isosch"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/sch:* and    not(@scheme='isoschematron')"
             "\x{a}" ~
             "	Rules in the ISO Schematron language must be inside\x{a}" ~
             "	a constraint with a value of 'isoschematron' on the scheme attribute\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    boho_att.global.attributes,
    boho_att.identified.attributes,
    boho_att.typed.attributes,
    ## supplies the name of the language in which the constraints
    ##       are defined
    attribute scheme {
      ##  (Schematron) 
      "schematron" | ##  (ISO Schematron) 
        "isoschematron" | ##  (XSLT) 
        "xsl" | ##  (private constraint language) 
        "private"
    },
    empty
  }

boho_attList =
  ## contains documentation for all the attributes associated with this element, as a series of
  ##       attDef elements. [22.4.4. Element Specifications 22.4.6. Element Classes]
  element ns1:attList {
    (boho_attRef | boho_attDef | boho_attList)+,
    boho_att.global.attributes,
    ## (organization) specifies whether all the attributes in the list are available (org="group") or only one
    ##         of them (org="choice")
    [ a:defaultValue = "group" ]
    attribute org {
      ## grouped
      "group" | ## alternated
        "choice"
    }?,
    empty
  }

boho_attDef =
  ## (attribute definition) contains the definition of a single attribute. [22.4.5. Attribute List Specification]
  element ns1:attDef {
    (boho_model.glossLike*,
     boho_datatype?,
     boho_constraintSpec*,
     boho_defaultVal?,
     (boho_valList | boho_valDesc+)?,
     boho_exemplum*,
     boho_remarks*)
    >> sch:pattern [
         id = "attDef-constraint-attDefContents"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:attDef"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "   @mode eq 'change'       or @mode eq 'delete'       or tei:datatype       or tei:valList[@type='closed']       "
             "Attribute: the definition of the @"
             sch:value-of [ select = "@ident" ]
             " attribute in the "
             sch:value-of [ select = "ancestor::*[@ident][1]/@ident" ]
             "\x{a}" ~
             "                  "
             sch:value-of [ select = "' '" ]
             "\x{a}" ~
             "                  "
             sch:value-of [
               select = "local-name(ancestor::*[@ident][1])"
             ]
             " should have a closed valList or a datatype"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    boho_att.global.attributes,
    boho_att.identified.attributes,
    ## specifies the optionality of the attribute.
    [ a:defaultValue = "opt" ]
    attribute usage {
      ## (required) 
      "req" | ## (mandatory when applicable ) 
        "mwa" | ## (recommended ) 
        "rec" | ## (recommended when applicable ) 
        "rwa" | ## (optional ) 
        "opt"
    }?,
    ## (namespace) specifies the namespace to which this attribute belongs
    [ a:defaultValue = "http://www.tei-c.org/ns/1.0" ]
    attribute ns { boho_data.namespace }?,
    empty
  }

boho_attRef =
  ## (attribute pointer) points to the definition of an attribute or group of attributes.
  element ns1:attRef {
    empty,
    boho_att.global.attributes,
    ## the name of the pattern defining the attribute(s)
    attribute name { boho_data.word },
    empty
  }

boho_datatype =
  ## specifies the declared value for an attribute, by referring to
  ##   any datatype defined by the chosen schema language. [1.4.2. Datatype Macros 22.4.5. Attribute List Specification]
  element ns1:datatype {
    boho_macro.schemaPattern*,
    boho_att.global.attributes,
    ## (minimum number of occurences) indicates the minimum number of times this datatype may
    ##       occur in the specification of the attribute being defined
    [ a:defaultValue = "1" ] attribute minOccurs { boho_data.count }?,
    ## (maximum number of occurences) indicates the maximum number of times this datatype may
    ##       occur in the specification of the attribute being defined
    [ a:defaultValue = "1" ]
    attribute maxOccurs { boho_data.count | "unbounded" }?,
    empty
  }

boho_defaultVal =
  ## (default value) specifies the default declared value for an attribute. [22.4.5. Attribute List Specification]
  element ns1:defaultVal { text, boho_att.global.attributes, empty }

boho_valDesc =
  ## (value description) specifies any semantic or syntactic constraint on the value that
  ## an attribute may take, additional to the information carried by the
  ## datatype element. [22.4.5. Attribute List Specification]
  element ns1:valDesc {
    boho_macro.phraseSeq.limited,
    boho_att.global.attributes,
    boho_att.translatable.attributes,
    boho_att.combinable.attributes,
    empty
  }

boho_valItem =
  ## documents a single attribute-value within a list of possible
  ##   or mandatory items. [22.4.5. Attribute List Specification]
  element ns1:valItem {
    boho_model.glossLike*,
    boho_att.global.attributes,
    boho_att.combinable.attributes,
    ## specifies the attribute value concerned.
    attribute ident { text },
    empty
  }

boho_valList =
  ## (value list) contains one or more valItem elements defining possible values for an attribute. [22.4.5. Attribute List Specification]
  element ns1:valList {
    boho_valItem*,
    ## specifies the extensibility of the list of attribute values specified. 
    [ a:defaultValue = "open" ]
    attribute type {
      ## only the values specified are permitted.
      "closed" | ## (semi-open) all the values specified should be supported, but other values are legal and
        ##             software should have appropriate fallback processing for them. 
        "semi" | ## the values specified are sample values only.
        "open"
    },
    boho_att.global.attributes,
    boho_att.combinable.attributes,
    empty
  }
boho_att.combinable.attributes = boho_att.combinable.attribute.mode
boho_att.combinable.attribute.mode =
  ## specifies the effect of this declaration on its parent
  ##     object.
  [ a:defaultValue = "add" ]
  attribute mode {
    ## this declaration is added to the current definitions
    "add" | ## if present already, the whole of the declaration 
      ##	  for this object is  removed from the current setup
      "delete" | ## this declaration changes the declaration of the same
      ##     name in the current definition
      "change" | ## this declaration replaces the declaration of the same
      ##     name in the current definition
      "replace"
  }?
boho_att.identified.attributes =
  boho_att.combinable.attributes,
  boho_att.identified.attribute.module,
  boho_att.identified.attribute.ident,
  boho_att.identified.attribute.predeclare,
  boho_att.identified.attribute.status

boho_att.identified.attribute.module =
  attribute module {
    "analysis" | "certainty" | "core" | "corpus" | "dictionaries" | "drama" | "figures" | "gaiji" | "header" | "iso-fs" | "linking" | "msdescription" | "namesdates" | "nets" | "spoken" | "tagdocs" | "tei" | "textcrit" | "textstructure" | "transcr" | "verse"
  }?
boho_att.identified.attribute.ident =
  ## Supplies the identifier by which this element may be referenced.
  attribute ident { boho_data.name }
boho_att.identified.attribute.predeclare =
  ## Says whether this object should be predeclared in the
  ##       tei infrastructure module.
  [ a:defaultValue = "false" ]
  attribute predeclare { boho_data.truthValue }?
boho_att.identified.attribute.status =
  ## indicates the current status of the object identified with
  ## respect to the current version of the TEI Guidelines.
  [ a:defaultValue = "stable" ]
  attribute status {
    ## the item is not recommended for use,
    ## and may be withdrawn at a future release.
    "deprecated" | ## the item is new and still under
      ## review.
      "unstable" | ## the item has changed significantly since the
      ## preceding version.
      "changed" | ## the item has not recently changed and is
      ## not expected to do so except for correction of any errors.
      "stable"
  }?
boho_att.datable.custom.attributes =
  boho_att.datable.custom.attribute.when-custom,
  boho_att.datable.custom.attribute.notBefore-custom,
  boho_att.datable.custom.attribute.notAfter-custom,
  boho_att.datable.custom.attribute.from-custom,
  boho_att.datable.custom.attribute.to-custom,
  boho_att.datable.custom.attribute.datingPoint,
  boho_att.datable.custom.attribute.datingMethod
boho_att.datable.custom.attribute.when-custom =
  ## supplies the value of a date or time in some standard form.
  attribute when-custom {
    list { boho_data.word, boho_data.word* }
  }?
boho_att.datable.custom.attribute.notBefore-custom =
  ## specifies the earliest possible date for the event in some custom standard form.
  attribute notBefore-custom {
    list { boho_data.word, boho_data.word* }
  }?
boho_att.datable.custom.attribute.notAfter-custom =
  ## specifies the latest possible date for the event in some custom standard form.
  attribute notAfter-custom {
    list { boho_data.word, boho_data.word* }
  }?
boho_att.datable.custom.attribute.from-custom =
  ## indicates the starting point of the period in some standard form.
  attribute from-custom {
    list { boho_data.word, boho_data.word* }
  }?
boho_att.datable.custom.attribute.to-custom =
  ## indicates the ending point of the period in some standard form.
  attribute to-custom {
    list { boho_data.word, boho_data.word* }
  }?
boho_att.datable.custom.attribute.datingPoint =
  ## supplies a pointer to some location defining a named point in time with reference to which the datable item is understood to have occurred
  attribute datingPoint { boho_data.pointer }?
boho_att.datable.custom.attribute.datingMethod =
  ## supplies a pointer to a calendarDesc element or other means of interpreting the values of the custom dating attributes.
  attribute datingMethod { boho_data.pointer }?
boho_att.datable.iso.attributes =
  boho_att.datable.iso.attribute.when-iso,
  boho_att.datable.iso.attribute.notBefore-iso,
  boho_att.datable.iso.attribute.notAfter-iso,
  boho_att.datable.iso.attribute.from-iso,
  boho_att.datable.iso.attribute.to-iso
boho_att.datable.iso.attribute.when-iso =
  ## supplies the value of a date or time in a standard form.
  attribute when-iso { boho_data.temporal.iso }?
boho_att.datable.iso.attribute.notBefore-iso =
  ## specifies the earliest possible date for the event in standard form, e.g. yyyy-mm-dd.
  attribute notBefore-iso { boho_data.temporal.iso }?
boho_att.datable.iso.attribute.notAfter-iso =
  ## specifies the latest possible date for the event in standard form, e.g. yyyy-mm-dd.
  attribute notAfter-iso { boho_data.temporal.iso }?
boho_att.datable.iso.attribute.from-iso =
  ## indicates the starting point of the period in standard form.
  attribute from-iso { boho_data.temporal.iso }?
boho_att.datable.iso.attribute.to-iso =
  ## indicates the ending point of the period in standard form.
  attribute to-iso { boho_data.temporal.iso }?

boho_orgName =
  ## (organization name) contains an organizational name.  [13.2.2. Organizational Names]
  element ns1:orgName {
    boho_macro.phraseSeq,
    boho_att.global.attributes,
    boho_att.datable.attributes,
    boho_att.editLike.attributes,
    boho_att.personal.attributes,
    boho_att.typed.attributes,
    empty
  }

boho_persName =
  ## (personal name) contains a proper noun or proper-noun phrase referring to a
  ##   person, possibly including one or more of
  ##   the person's forenames, surnames, honorifics, added names, etc. [13.2.1. Personal Names]
  element ns1:persName {
    boho_macro.phraseSeq,
    boho_att.global.attributes,
    boho_att.datable.attributes,
    boho_att.editLike.attributes,
    boho_att.personal.attributes,
    boho_att.typed.attributes,
    empty
  }

boho_placeName =
  ## contains an absolute or relative place name.  [13.2.3. Place Names]
  element ns1:placeName {
    boho_macro.phraseSeq,
    boho_att.datable.attributes,
    boho_att.editLike.attributes,
    boho_att.global.attributes,
    boho_att.naming.attributes,
    boho_att.typed.attributes,
    empty
  }

boho_table =
  ## contains text displayed in tabular form, in rows and columns. [14.1.1. TEI Tables]
  element ns1:table {
    ((boho_model.headLike | boho_model.global)*,
     ((boho_row, boho_model.global*)+
      | (boho_model.graphicLike, boho_model.global*)+)),
    boho_att.global.attributes,
    ## indicates the number of rows in the table.
    attribute rows { boho_data.count }?,
    ## (columns) indicates the number of columns in each row of the table.
    attribute cols { boho_data.count }?,
    empty
  }

boho_row =
  ## contains one row of a table.  [14.1.1. TEI Tables]
  element ns1:row {
    boho_cell+,
    boho_att.global.attributes,
    boho_att.tableDecoration.attributes,
    empty
  }

boho_cell =
  ## contains one cell of a table.  [14.1.1. TEI Tables]
  element ns1:cell {
    boho_macro.paraContent,
    boho_att.global.attributes,
    boho_att.tableDecoration.attributes,
    empty
  }

boho_formula =
  ## contains a mathematical or other formula. [14.2. Formulæ and
  ## Mathematical Expressions]
  element ns1:formula {
    (text | boho_model.graphicLike | boho_model.hiLike)*,
    boho_att.global.attributes,
    ## names the notation used for the content of the element.
    attribute notation { boho_data.enumerated }?,
    empty
  }

boho_figure =
  ## groups elements representing or containing graphic information
  ##   such as an illustration, formula,  or    figure. [14.4. Specific Elements for Graphic Images]
  element ns1:figure {
    (boho_model.headLike
     | boho_model.common
     | boho_figDesc
     | boho_model.graphicLike
     | boho_model.global
     | boho_model.divBottomPart)*,
    boho_att.global.attributes,
    boho_att.placement.attributes,
    boho_att.typed.attributes,
    empty
  }

boho_figDesc =
  ## (description of figure) contains a brief prose description of the appearance or content
  ## of a graphic figure, for use when documenting an image without
  ## displaying it. [14.4. Specific Elements for Graphic Images]
  element ns1:figDesc {
    boho_macro.limitedContent, boho_att.global.attributes, empty
  }

boho_fallback =
  element ns2:fallback { (text | boho_macro.anyXML)+ }
start =
  boho_TEI
  | boho_body
  | boho_schemaSpec
  | boho_elementSpec
  | boho_classSpec
  | boho_macroSpec
  | boho_div
