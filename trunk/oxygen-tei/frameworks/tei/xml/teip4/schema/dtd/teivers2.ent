<!--First, declare the class of
components specific to verse -->
<!ENTITY % x.comp.verse "" >
<!ENTITY % m.comp.verse "%x.comp.verse; %n.lg1; | %n.lg2; | %n.lg3; | %n.lg4; | %n.lg5;"> 
<!ENTITY % mix.verse '| %m.comp.verse;' >
<!--Next define attributes common to metrical elements-->
<!ENTITY % a.metrical '
      met CDATA %INHERITED;
      real CDATA #IMPLIED
      rhyme CDATA #IMPLIED'> 
<!ENTITY % a.enjamb '
      enjamb CDATA #IMPLIED'> 
